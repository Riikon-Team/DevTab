{
  "manifest_version": 3,
  "name": "DevTab",
  "description": "A new tab page for developers",
  "version": "1.0.4",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": []
}